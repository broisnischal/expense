{
  "name": "my-hono-app",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "generate": "npx drizzle-kit generate",
    "migrate": "npx wrangler d1 migrations create expense --name new",
    "migrate:apply": "npx wrangler d1 migrations apply expense --remote",
    "migrate:local": "npx wrangler d1 migrations apply expense --local",
    "seed": "bun run ./src/drizzle/seed.ts",
    "studio": "npx drizzle-kit studio",
    "drop": "npx drizzle-kit drop",
    "up": "drizzle-kit up:sqlite --schema=./app/drizzle/schema.server.ts"
  },
  "dependencies": {
    "@hono-rate-limiter/cloudflare": "^0.1.2",
    "@hono-rate-limiter/redis": "^0.1.4",
    "@hono/sentry": "^1.2.0",
    "@hono/swagger-ui": "^0.4.0",
    "@hono/zod-openapi": "^0.15.3",
    "@hono/zod-validator": "^0.2.2",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@lucia-auth/adapter-sqlite": "^3.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/qs": "^6.9.15",
    "@types/uuid": "^10.0.0",
    "@upstash/ratelimit": "^2.0.1",
    "@upstash/redis": "^1.34.0",
    "@upstash/vector": "^1.1.5",
    "axios": "^1.7.7",
    "better-sqlite3": "^11.1.2",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "hono": "^4.5.5",
    "hono-rate-limiter": "^0.4.0",
    "lucia": "^3.2.0",
    "nvm": "^0.0.4",
    "oslo": "^1.2.1",
    "qs": "^6.13.0",
    "up-fetch": "^2.1.8",
    "uuid": "^10.0.0",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240806.0",
    "@types/node": "^22.2.0",
    "drizzle-kit": "^0.24.0",
    "wrangler": "4"
  }
}
